<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane id="AnchorPane" prefHeight="277.0" prefWidth="394.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.111" fx:controller="converter.FXMLDocumentController">
   <children>
      <TabPane layoutX="14.0" layoutY="14.0" prefHeight="200.0" prefWidth="350.0" tabClosingPolicy="UNAVAILABLE">
        <tabs>
          <Tab text="Length">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="14.0" layoutY="32.0" text="Meter" />
                        <Label layoutX="18.0" layoutY="86.0" text="Centimeter" />
                        <TextField fx:id="meterField" layoutX="91.0" layoutY="28.0" />
                        <TextField fx:id="centimeterField" layoutX="91.0" layoutY="82.0" />
                        <Button layoutX="175.0" layoutY="145.0" mnemonicParsing="false" onAction="#handleConvertAction" text="Convert" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
          <Tab fx:id="massField" text="Mass">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label layoutX="3.0" layoutY="30.0" text="Inches" />
                        <Label layoutX="3.0" layoutY="77.0" text="Kilometers" />
                        <TextField fx:id="inchesField" layoutX="74.0" layoutY="26.0" />
                        <TextField fx:id="kilometersField" layoutX="74.0" layoutY="73.0" />
                        <Button layoutX="188.0" layoutY="132.0" mnemonicParsing="false" onAction="#handleMessConvertAction" text="Convert" />
                     </children>
                  </AnchorPane>
            </content>
          </Tab>
        </tabs>
      </TabPane>
   </children>
</AnchorPane>
